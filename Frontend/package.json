{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "author": "Tarbadev",
  "homepage": "https://github.com/tarbadev/witchcraft#readme",
  "main": "src/index.js",
  "scripts": {
    "start": "node server.js",
    "localServer": "webpack-dev-server --hot --watch --open --color --mode development",
    "build": "npm-run-all --parallel lint compile",
    "build:watch": "npm-run-all --parallel lint:watch compile:watch",
    "compile": "./node_modules/.bin/webpack --mode production",
    "compile:watch": "npm run compile -- --watch",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/system": "^4.0.1",
    "connect-history-api-fallback": "^1.6.0",
    "connected-react-router": "4.5.0",
    "current-week-number": "^1.0.7",
    "express": "^4.17.1",
    "history": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "latest",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "updeep": "1.0.0",
    "weeknumber": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.9",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": "^5.16.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-watch": "^4.0.2",
    "fetch-mock": "^7.3.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "jest-junit": "^6.4.0",
    "jest-resolve": "^24.8.0",
    "jest-transform-stub": "^1.0.0",
    "jest-webpack": "^0.5.1",
    "node-fetch": "^2.6.0",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.21.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "jest": {
    "resetMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupUnitTests.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^test-resources(.*)$": "<rootDir>/test_resources$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "jest-junit": {
    "suiteName": "Frontend Tests",
    "outputDirectory": "<rootDir>/build/test-results",
    "outputName": "react.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "engines": {
    "node": "12.3.1",
    "npm": "6.9.0"
  }
}
