{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "author": "Tarbadev",
  "homepage": "https://github.com/tarbadev/witchcraft#readme",
  "main": "src/index.js",
  "scripts": {
    "start": "node server.js",
    "localServer": "webpack-dev-server --hot --watch --open --color --mode development --host 0.0.0.0",
    "build": "npm-run-all --parallel lint compile",
    "build:watch": "npm-run-all --parallel lint:watch compile:watch",
    "compile": "./node_modules/.bin/webpack --mode production",
    "compile:watch": "npm run compile -- --watch",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.42",
    "@material-ui/styles": "^4.9.0",
    "@material-ui/system": "^4.9.1",
    "conditional-expression": "^1.1.2",
    "connect-history-api-fallback": "^1.6.0",
    "convert-units": "^2.3.4",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-swipeable-views": "^0.13.9",
    "updeep": "^1.2.0",
    "weeknumber": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^2.6.11",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-watch": "^4.0.2",
    "fetch-mock": "^7.7.3",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^6.4.0",
    "jest-resolve": "^24.9.0",
    "jest-transform-stub": "^1.0.0",
    "jest-webpack": "^0.5.1",
    "node-fetch": "^3.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "resetMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupUnitTests.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^test-resources(.*)$": "<rootDir>/test_resources$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "jest-junit": {
    "suiteName": "Frontend Tests",
    "outputDirectory": "<rootDir>/build/test-results",
    "outputName": "react.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "engines": {
    "node": "13.8.0",
    "npm": "6.13.6"
  }
}
