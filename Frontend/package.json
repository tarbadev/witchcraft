{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "author": "Tarbadev",
  "homepage": "https://github.com/tarbadev/witchcraft#readme",
  "main": "src/index.js",
  "scripts": {
    "start": "node server.js",
    "localServer": "webpack-dev-server --hot --watch --open --color --mode development",
    "build": "npm-run-all --parallel lint compile",
    "build:watch": "npm-run-all --parallel lint:watch compile:watch",
    "compile": "./node_modules/.bin/webpack --mode production",
    "compile:watch": "npm run compile -- --watch",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.1",
    "connected-react-router": "4.5.0",
    "current-week-number": "^1.0.7",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "updeep": "1.0.0",
    "weeknumber": "^1.1.1",
    "prop-types": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.5.7",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.8.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "fetch-mock": "^7.2.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "jest-junit": "^5.2.0",
    "jest-transform-stub": "^1.0.0",
    "jest-webpack": "^0.5.1",
    "react-test-renderer": "^16.4.1",
    "redux-devtools-extension": "^2.13.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "resetMocks": true,
    "setupTestFrameworkScriptFile": "<rootDir>src/tests.js",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1",
      "^test-resources(.*)$": "<rootDir>/test_resources$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "jest-junit": {
    "suiteName": "React Tests",
    "outputDirectory": "build/test-results",
    "outputName": "react.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  }
}
